z <- OneDimensionTextureHilbert(img)
gamma.parameters[[j]] <- fitdistr(z, "gamma")
gamma.results[j,] = c(unlist(gamma.parameters[[j]])[1], unlist(gamma.parameters[[j]])[2], unlist(gamma.parameters[[j]])[3], unlist(gamma.parameters[[j]])[4])
